should match snapshot for certificate configuration:
  1: |
    replicas: 1
    selector:
      matchLabels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/name: kairoscloud-chart
    template:
      metadata:
        labels:
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: kairoscloud-chart
      spec:
        containers:
          - image: nginx:1.0.0
            imagePullPolicy: IfNotPresent
            livenessProbe:
              enabled: true
              failureThreshold: 3
              httpGet:
                path: /health
                port: http
              initialDelaySeconds: 30
              periodSeconds: 10
              timeoutSeconds: 5
            name: kairoscloud-chart
            ports:
              - containerPort: 8080
                name: http
                protocol: TCP
            readinessProbe:
              enabled: true
              failureThreshold: 3
              httpGet:
                path: /ready
                port: http
              initialDelaySeconds: 5
              periodSeconds: 5
              timeoutSeconds: 3
            resources:
              limits:
                cpu: 500m
                memory: 512Mi
              requests:
                cpu: 250m
                memory: 256Mi
            securityContext:
              capabilities:
                drop:
                  - ALL
              readOnlyRootFilesystem: true
              runAsNonRoot: true
              runAsUser: 1000
        securityContext:
          fsGroup: 2000
        serviceAccountName: RELEASE-NAME-kairoscloud-chart
  2: |
    ports:
      - name: http
        port: 80
        protocol: TCP
        targetPort: 8080
    selector:
      app.kubernetes.io/instance: RELEASE-NAME
      app.kubernetes.io/name: kairoscloud-chart
    type: ClusterIP
  3: |
    dnsNames:
      - example.com
    issuerRef:
      group: cert-manager.io
      kind: ClusterIssuer
      name: letsencrypt-prod
    privateKey:
      algorithm: RSA
      size: 2048
    secretName: RELEASE-NAME-kairoscloud-chart-tls
